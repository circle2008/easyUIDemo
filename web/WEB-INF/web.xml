<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>student</display-name>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>gradeListServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.gradeListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gradeListServlet</servlet-name>
        <url-pattern>/gradeList</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>gradeDeleteServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.gradeDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gradeDeleteServlet</servlet-name>
        <url-pattern>/gradeDelete</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>gradeAddUpdateServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.gradeAddUpadateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gradeAddUpdateServlet</servlet-name>
        <url-pattern>/gradeAddUpdate</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>studentListServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.studentListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentListServlet</servlet-name>
        <url-pattern>/studentList</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>gradeComboxListServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.gradeComboListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gradeComboxListServlet</servlet-name>
        <url-pattern>/gradeComboList</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>studentDeleteServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.studentDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentDeleteServlet</servlet-name>
        <url-pattern>/studentDelete</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>studentAddUpdateServlet</servlet-name>
        <servlet-class>com.dlmu.circle.web.studentAddUpadateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentAddUpdateServlet</servlet-name>
        <url-pattern>/studentAddUpdate</url-pattern>
    </servlet-mapping>
</web-app>